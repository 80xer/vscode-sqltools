// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/dist/": true, // set this to true to hide the "dost" folder with the compiled JS files
    "**/.vscode-test/": true,
    "**/.nyc_output/": true
  },
  "search.exclude": {
    "**/out/": true, // set this to false to include "dost" folder in search results
    "**/.vscode-test/": true,
    "**/.nyc_output/": true
  },
  "eslint.enable": false,
  "standard.enable": false
}
